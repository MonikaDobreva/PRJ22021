<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.FlightController">
   <children>
      <VBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
              <fx:include fx:id="menuBar" source="topMenu.fxml" />
            <VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="385.0" prefWidth="480.0">
               <children>
                    <HBox alignment="CENTER_LEFT" prefHeight="18.0" prefWidth="400.0">
                        <children>
                            <Label alignment="CENTER" text="Register a flight" textFill="#4f78b0">
                                <font>
                                    <Font name="Calibri Bold Italic" size="25.0" />
                                </font>
                            </Label>
                        </children>
                        <padding>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                        </padding>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                    </HBox>
                  <Separator prefHeight="0.0" prefWidth="400.0" />
                    <GridPane prefHeight="204.0" prefWidth="461.0">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="115.0" minWidth="10.0" prefWidth="106.0" />
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="178.0" />
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints maxHeight="33.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="40.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="40.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        </rowConstraints>
                        <children>
                            <Label prefHeight="17.0" prefWidth="150.0" text="Departure Time:" />
                        <HBox alignment="CENTER_LEFT" spacing="10.0" GridPane.columnIndex="1">
                           <children>
                              <Spinner fx:id="depTimeHourSpinner" editable="true" max="23" min="00" prefHeight="25.0" prefWidth="59.0" />
                              <Spinner fx:id="depTimeMinSpinner" editable="true" max="59" min="00" prefHeight="25.0" prefWidth="58.0" />
                              <DatePicker fx:id="depTimePicker" promptText="select date" showWeekNumbers="true" />
                           </children>
                        </HBox>
                            <Label prefHeight="17.0" prefWidth="132.0" text="Arrival Time:" GridPane.rowIndex="1" />
                        <HBox alignment="CENTER_LEFT" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <children>
                              <Spinner fx:id="arrTimeHourSpinner" editable="true" max="23" min="00" prefHeight="25.0" prefWidth="60.0" />
                              <Spinner fx:id="arrTimeMinSpinner" editable="true" max="59" min="00" prefHeight="25.0" prefWidth="60.0" />
                                <DatePicker fx:id="arrTimePicker" promptText="select date" showWeekNumbers="true" />
                            </children>
                        </HBox>
                        <Label text="Airplane Model" GridPane.rowIndex="4" />
                        <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                           <children>
                              <TextField fx:id="airplaneModelField" prefHeight="25.0" prefWidth="150.0" promptText="e.g. &quot;Boeing 747&quot;" text="Boeing 747" />
                              <ComboBox fx:id="airplaneModelDropdown" onMouseClicked="#listAirplaneModels" prefHeight="25.0" prefWidth="233.0" promptText="select Airplane Model" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                           <children>
                              <TextField fx:id="destinationApField" promptText="IATA airport code" text="DUS">
                                 <font>
                                    <Font name="System Italic" size="12.0" />
                                 </font>
                              </TextField>
                              <ComboBox fx:id="destinationApDropdown" onMouseClicked="#listDestinationAirports" prefHeight="25.0" prefWidth="229.0" promptText="select Destination" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
                           <children>
                              <TextField fx:id="basePriceField" prefHeight="25.0" prefWidth="69.0" promptText="IATA airport code" text="90">
                                  <font>
                                      <Font name="System Italic" size="12.0" />
                                  </font>
                              </TextField>
                              <Label text="â‚¬" />
                           </children>
                        </HBox>
                        <Label text="Destination" GridPane.rowIndex="3" />
                        <Label text="Base Price" GridPane.rowIndex="5" />
                        <Label layoutX="10.0" layoutY="156.0" text="Origin" GridPane.rowIndex="2" />
                        <HBox alignment="CENTER_LEFT" layoutX="125.0" layoutY="148.0" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                           <children>
                              <TextField fx:id="originApField" prefHeight="25.0" prefWidth="104.0" promptText="IATA airport code" text="DUS">
                                 <font>
                                    <Font name="System Italic" size="12.0" />
                                 </font>
                              </TextField>
                              <ComboBox fx:id="originApDropdown" onMouseClicked="#listOriginAirports" prefHeight="25.0" prefWidth="185.0" promptText="select Origin" />
                           </children>
                        </HBox>
                        </children>
                    </GridPane>
                    <HBox alignment="CENTER" prefHeight="25.0" prefWidth="456.0" spacing="20.0">
                        <children>
                        <Label fx:id="nfcLabel" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="185.0" />
                            <Button fx:id="ShowFlights" onAction="#switchToViewFlight" text="Show Flights" />
                            <Button fx:id="StoreFlight" contentDisplay="CENTER" onAction="#storeFlight" prefHeight="25.0" prefWidth="99.0" text="Register flight" />
                        </children>
                    </HBox>
                  <HBox>
                     <children>
                          <Button fx:id="primaryButton" onAction="#backToStart" text="Back" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
